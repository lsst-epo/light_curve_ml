{
  "__Generic pipeline params__": null,
  "globalParams": {
    "places": 3,
    "dataLimit": null,
    "trainSize": 0.80
  },
  "database": {
    "timeout": 300,
    "raw_lc_table": "raw_lcs",
    "clean_lc_table": "clean_lcs",
    "feature_table": "lc_features",
    "commitFrequency": 2000,
    "pageSize": 1000
  },
  "loadData": {
    "params": {
      "skiprows": 1,
      "stdLimit": 5,
      "errorLimit": 3
    }
  },
  "extractFeatures": {
    "function": "feets",
    "params": {
      "excludedFeatures": ["CAR_mean", "CAR_sigma", "CAR_tau"],
      "impute": true
    }
  },
  "modelSearch": {
    "function": "grid",
    "params": {
        "model": "sklearn.ensemble.RandomForestClassifier",
        "classWeight": "balanced",
        "treesValues": [250, 500, 750, 1000],
        "maxFeaturesValues": ["sqrt", "log2"],
        "folds": 10,
        "repeats": 1,
        "jobs": -1
    }
  },
  "serialization": {
    "params": {
    }
  }
}
