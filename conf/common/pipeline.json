{
  "__Default pipeline params__": null,
  "globalParams": {
    "places": 3,
    "trainSize": 0.80
  },
  "database": {
    "raw_lc_table": "raw_lcs",
    "clean_lc_table": "clean_lcs",
    "feature_table": "lc_features",
    "timeout": 300,
    "commitFrequency": 200,
    "pageSize": 100
  },
  "loadData": {
    "params": {
      "skiprows": 1,
      "stdLimit": 5,
      "errorLimit": 3
    }
  },
  "extractFeatures": {
    "function": "feets",
    "params": {
      "skip": false,
      "offset": 0,
      "excludedFeatures": [],
      "impute": true
    }
  },
  "modelSearch": {
    "function": "grid",
    "params": {
        "model": "sklearn.ensemble.RandomForestClassifier",
        "classWeight": "balanced",
        "n_trees": [250, 500, 750, 1000],
        "max_features": ["sqrt", "log2"],
        "folds": 5,
        "repeats": 1,
        "jobs": -1
    }
  },
  "serialization": {
    "params": {
    }
  }
}
