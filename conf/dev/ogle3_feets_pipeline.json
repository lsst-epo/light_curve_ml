{
  "globalParams": {
    "places": 3
  },
  "database": {
    "dbPath": "data/ogle3/ogle3_processed.db",
    "timeout": 60,
    "raw_lc_table": "raw_lcs",
    "clean_lc_table": "clean_lcs",
    "feature_table": "lc_features",
    "commitFrequency": 1000,
    "pageSize": 1000
  },
  "loadData": {
    "function": "ogle3",
    "params": {
      "dataName": "ogle3",
      "relativePath": "/data/ogle3/ogle3.csv",
      "skiprows": 1,
      "stdLimit": 5,
      "errorLimit": 3,
      "skip": false
    }
  },
  "extractFeatures": {
    "function": "feets",
    "params": {
      "excludedFeatures": ["CAR_mean", "CAR_sigma", "CAR_tau"],
      "impute": true,
      "skip": false
    }
  },
  "modelSelection": {
    "function": "grid",
    "params": {
        "estimatorsStart": 5,
        "estimatorsStop": 15,
        "rfFeaturesStart": 5,
        "rfFeaturesStop": 15,
        "cv": 5,
        "jobs": -1
    }
  },
  "serialization": {
    "params": {
      "loadPath": null,
      "savePath": "/home/ubuntu/light_curve_ml/models/ogle3/rf-classifier-ogle3-full.pkl"
    }
  }
}
