{
  "globalParams": {
    "type": "supervised"
  },
  "database": {
    "dbPath": "/data/db/macho/macho_processed.db",
    "commitFrequency": 200,
    "pageSize": 100
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "/data/datasets/macho/macho-train.csv",
      "filter": [-99.0],
      "skip": true,
      "transform": false
    }
  },
  "extractFeatures": {
    "params": {
      "excludedFeatures": [],
      "skip": true
    }
  },
  "modelSearch": {
    "function": "grid",
    "model": {
      "class": "sklearn.ensemble.RandomForestClassifier",
      "params": {
        "n_jobs": -1,
        "class_weight": "balanced"
      }
    },
    "params": {
        "cv": {
          "n_splits": 5,
          "n_repeats": 1
        },
        "gridSearch": {
          "param_grid": {
            "n_estimators": [500, 700, 900, 1100],
            "criterion": ["gini", "entropy"],
            "max_features": [9, 12, 18],
            "min_samples_leaf": [1, 2, 3]
          },
          "scoring": "f1_weighted",
          "pre_dispatch": "10 * n_jobs",
          "n_jobs": -1,
          "iid": true,
          "verbose": 2,
          "error_score": "raise"
        }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/data/models/macho/supervised/rf-classifier-macho-train.pkl",
      "imgPath": "/data/models/macho/supervised"
    }
  }
}
