{
  "globalParams": {
    "type": "supervised"
  },
  "database": {
    "dbPath": "/data/db/macho/macho_processed.db",
    "commitFrequency": 200,
    "pageSize": 100
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "/data/datasets/macho/macho-train.csv",
      "filter": [-99.0],
      "skip": true
    }
  },
  "extractFeatures": {
    "params": {
      "excludedFeatures": []
    }
  },
  "modelSearch": {
    "function": "grid",
    "params": {
        "folds": 10,
        "treesValues": [900, 1000, 1100, 1200],
        "maxFeaturesValues": ["sqrt", "log2"]
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/data/models/macho/supervised/rf-classifier-macho-train.pkl",
      "imgPath": "/data/models/macho/supervised"
    }
  }
}
