{
  "__OGLE3 dataset, supervised, for Nix deploy__": null,
  "globalParams": {
    "type": "supervised"
  },
  "database": {
    "dbPath": "/data/db/ogle3/ogle3_processed.db",

    "raw_lc_table": "raw_lcs",
    "clean_lc_table": "std_clean_lcs",
    "feature_table": "std_lc_features"
  },
  "loadData": {
    "skip": false,
    "params": {
      "dataName": "ogle3",
      "relativePath": "/data/datasets/ogle3/ogle3.csv"
    },
    "writeTable": "raw_lc_table"
  },
  "preprocessData": {
    "skip": false,
    "params": {
      "standardize": true
    },
    "writeTable": "clean_lc_table"
  },
  "extractFeatures": {
    "skip": false,
    "params": {
      "excludedFeatures": [],
      "standardize": false
    },
    "writeTable": "feature_table"
  },
  "postprocessFeatures": {
    "skip": true,
    "params": {
    }
  },
  "modelSearch": {
    "function": "grid",
    "model": {
      "class": "sklearn.ensemble.RandomForestClassifier",
      "params": {
        "n_jobs": -1,
        "class_weight": "balanced"
      }
    },
    "params": {
        "cv": {
          "n_splits": 5,
          "n_repeats": 1
        },
        "gridSearch": {
          "param_grid": {
            "n_estimators": [50, 100, 250, 500, 1000],
            "criterion": ["gini", "entropy"],
            "max_features": [3, 9, 18],
            "min_samples_leaf": [1, 2, 3]
          },
          "scoring": "f1_weighted",
          "pre_dispatch": "10 * n_jobs",
          "n_jobs": -1,
          "iid": true,
          "verbose": 2,
          "error_score": "raise"
        }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/data/models/ogle3/supervised/rf-classifier-ogle3-full.pkl",
      "imgPath": "/data/models/ogle3/supervised"
    }
  }
}
