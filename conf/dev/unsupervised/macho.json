{
  "globalParams": {
    "type": "unsupervised",
    "trainSize": 1.0
  },
  "database": {
    "dbPath": "/data/db/macho/macho_processed.db"
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "data/macho/macho-train.csv",
      "skip": true,

      "skipCleaning": true,
      "filter": [-99.0],
      "standardize": false
    }
  },
  "extractFeatures": {
    "params": {
      "skip": true
    }
  },
  "modelSearch": {
    "function": "grid",
    "params": {
      "componentsStart": 5,
      "componentsStop": 41,
      "componentsStep": 2,
      "clusterValues": [10, 11, 12],
      "miniBatchKMeansArgs": {
        "init": "k-means++",
        "max_iter": 1000,
        "batch_size": 5000,
        "verbose": false,
        "max_no_improvement": 1000,
        "n_init": 5000,
        "reassignment_ratio": 1
      },
      "kmeansArgs": {
        "init": "k-means++",
        "max_iter": 1000,
        "verbose": false,
        "n_init": 5000,
        "precompute_distances": "auto",
        "n_jobs": -1,
        "algorithm": "full"
      },
      "agglomerativeArgs": {
        "linkages": {"agglomerative (ward)": "ward"},
        "memory": "/data/tmp"
      }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/data/models/macho/unsupervised/classifier-macho.pkl",
      "imgPath": "/data/models/macho/unsupervised"
    }
  }
}
