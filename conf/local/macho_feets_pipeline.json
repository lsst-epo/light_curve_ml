{
  "globalParams": {
    "places": 3
  },
  "database": {
    "dbPath": "data/ogle3/ogle3_processed.db",
    "raw_lc_table": "raw_lcs",
    "clean_lc_table": "clean_lcs",
    "feature_table": "lc_features",
    "commitFrequency": 500
  },
  "loadData": {
    "function": "macho",
    "params": {
      "relativePath": "data/macho/macho-train.csv",
      "skiprows": 1,
      "stdLimit": 5,
      "errorLimit": 3,
      "filter": [-99.0]
    }
  },
  "extractFeatures": {
    "function": "feets",
    "params": {
      "excludedFeatures": ["CAR_mean", "CAR_sigma", "CAR_tau"],
      "impute": true
    }
  },
  "modelSelection": {
    "function": "grid",
    "params": {
        "estimatorsStart": 6,
        "estimatorsStop": 8,
        "rfFeaturesStart": 6,
        "rfFeaturesStop": 8,
        "cv": 5,
        "jobs": -1
    }
  },
  "serialization": {
    "params": {
      "xloadPath": "/Users/ryanjmccall/code/light_curve_ml/models/ogle3/rf-classifier.pkl",
      "loadPath": null,
      "savePath": "/Users/ryanjmccall/code/light_curve_ml/models/ogle3/rf-classifier-macho.pkl"
    }
  }
}
