{
  "globalParams": {
    "places": 3
  },
  "loadData": {
    "function": "ogle3",
    "params": {
      "skiprows": 1,
      "batchSize": 10000,
      "relativePath": "data/ogle3/ogle3-sample.csv",
      "dbPath": "data/ogle3/ogle3_processed.db",
      "raw_lc_table": "raw_lcs",
      "clean_lc_table": "clean_lcs",
      "feature_table": "lc_features",
      "columns": ["id", "label", "times", "magnitudes", "errors"]
    }
  },
  "extractFeatures": {
    "function": "feets",
    "params": {
      "feature_table": "lc_features",
      "allFeatures": false,
      "impute": true
    }
  },
  "modelSelection": {
    "function": "grid",
    "params": {
        "estimatorsStart": 6,
        "estimatorsStop": 8,
        "rfFeaturesStart": 6,
        "rfFeaturesStop": 8,
        "cv": 5,
        "jobs": -1
    }
  },
  "serialization": {
    "params": {
      "xloadPath": "/Users/ryanjmccall/code/light_curve_ml/models/ogle3/rf-classifier.pkl",
      "loadPath": null,
      "savePath": "/Users/ryanjmccall/code/light_curve_ml/models/ogle3/rf-classifier-new.pkl"
    }
  }
}
