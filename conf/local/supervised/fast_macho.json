  "database": {
    "dbPath": "data/macho/macho_processed.db",
    "raw_lc_table": "raw_lcs",
    "clean_lc_table": "clean_lcs",
    "feature_table": "lc_features"
  },
  "loadData": {
    "skip": false,
    "params": {
      "dataName": "macho",
      "relativePath": "data/macho/macho-train.csv"
    },
    "writeTable": "raw_lc_table"
  },
  "preprocessData": {
    "skip": false,
    "params": {
      "filter": [-99.0],
      "standardize": true
    },
    "writeTable": "clean_lcs"
  },
  "extractFeatures": {
    "skip": false,
    "params": {
      "offset": 0,
      "excludedFeatures": []
