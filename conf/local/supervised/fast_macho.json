  "postprocessFeatures": {
    "skip": false,
    "params": {
      "impute": true,
      "standardize": true
    },
    "writeTable": null
  },
  "modelSearch": {
    "function": "grid",
    "model": {
      "class": "sklearn.ensemble.RandomForestClassifier",
      "params": {
        "n_jobs": -1,
        "class_weight": "balanced"
