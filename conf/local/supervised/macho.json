{
  "__local super MACHO__": null,
  "globalParams": {
    "type": "supervised"
  },
  "database": {
    "dbPath": "data/macho/macho_processed.db",
    "clean_lc_table": "standardized_clean_lcs",
    "feature_table": "lc_standardized_features",
    "commitFrequency": 500,
    "pageSize": 500
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "data/macho/macho-train.csv",
      "skip": true,
      "skipCleaning": true,
      "filter": [-99.0],
      "standardize": true
    }
  },
  "extractFeatures": {
    "params": {
      "skip": false,
      "offset": 10,
      "excludedFeatures": []
    }
  },
  "modelSearch": {
    "function": "grid",
    "model": {
      "class": "sklearn.ensemble.RandomForestClassifier",
      "params": {
        "n_jobs": -1,
        "class_weight": "balanced"
      }
    },
    "params": {
        "cv": {
          "n_splits": 5,
          "n_repeats": 1
        },
        "gridSearch": {
          "param_grid": {
            "n_estimators": [50],
            "criterion": ["gini", "entropy"],
            "max_features": [3],
            "min_samples_leaf": [1]
          },
          "scoring": "f1_weighted",
          "pre_dispatch": "10 * n_jobs",
          "n_jobs": -1,
          "iid": true,
          "verbose": 1,
          "error_score": "raise"
        }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/supervised/lc_standardized_rf-classifier-macho.pkl",
      "imgPath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/supervised/lc_standardized"
    }
  }
}
