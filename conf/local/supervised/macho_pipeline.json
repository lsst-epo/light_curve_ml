{
  "globalParams": {
    "type": "supervised"
  },
  "database": {
    "dbPath": "data/macho/macho_processed.db"
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "data/macho/macho-train.csv",
      "filter": [-99.0],
      "skip": true
    }
  },
  "extractFeatures": {
    "params": {
      "skip": true
    }
  },
  "modelSearch": {
    "function": "grid",
    "model": {
      "class": "sklearn.ensemble.RandomForestClassifier",
      "params": {
        "n_jobs": -1,
        "class_weight": "balanced"
      }
    },
    "params": {
        "cv": {
          "n_splits": 5,
          "n_repeats": 1
        },
        "gridSearch": {
          "param_grid": {
            "n_estimators": [50, 100, 250, 900, 1000, 1100, 1200],
            "criterion": ["gini", "entropy"],
            "max_features": [3, 6, 9, 12, 18],
            "min_samples_leaf": [1, 2, 3]
          },
          "scoring": "f1_weighted",
          "pre_dispatch": "10 * n_jobs",
          "n_jobs": -1,
          "iid": true,
          "verbose": 2,
          "error_score": "raise"
        }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/supervised/rf-classifier-macho.pkl",
      "imgPath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/supervised"
    }
  }
}
