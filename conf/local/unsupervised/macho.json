{
  "globalParams": {
    "type": "unsupervised",
    "trainSize": 0.999999,
    "dataLimit": 400
  },
  "database": {
    "dbPath": "data/macho/macho_processed.db",
    "commitFrequency": 5000,
    "pageSize": 5000
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "data/macho/macho-train.csv",
      "skip": true,

      "skipCleaning": true,
      "filter": [-99.0],
      "standardize": false
    }
  },
  "extractFeatures": {
    "params": {
      "skip": true
    }
  },
  "modelSearch": {
    "function": "grid",
    "params": {
      "componentsStart": 2,
      "componentsStop": 63,
      "componentsStep": 30,
      "clusterValues": [10, 11, 12],
      "miniBatchKMeansArgs": {
        "init": "k-means++",
        "max_iter": 1000,
        "batch_size": 5000,
        "verbose": false,
        "max_no_improvement": 1000,
        "n_init": 5000,
        "reassignment_ratio": 1
      },
      "kmeansArgs": {
        "init": "k-means++",
        "max_iter": 1000,
        "verbose": false,
        "n_init": 5000,
        "precompute_distances": "auto",
        "n_jobs": -1,
        "algorithm": "full"
      },
      "agglomerativeArgs": {
        "memory": "/Users/ryanjmccall/code/light_curve_ml/tmp"
      }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/unsupervised/rf-classifier-macho.pkl",
      "imgPath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/unsupervised"
    }
  }
}
