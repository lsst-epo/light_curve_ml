        "verbose": false,
        "max_no_improvement": 100,

        "n_init": 10,
        "reassignment_ratio": 0.1
      },
      "agglomerativeArgs": {
        "affinity": "euclidean",
        "linkage": "ward"
      }
    }
  },
  "serialization": {
    "params": {
      "modelLoadPath": null,
      "modelSavePath": "/Users/ryanjmccall/code/light_curve_ml/models/macho/unsupervised/rf-classifier-macho.pkl",
