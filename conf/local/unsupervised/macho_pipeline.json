{
  "globalParams": {
    "type": "unsupervised"
  },
  "database": {
    "dbPath": "data/macho/macho_processed.db"
  },
  "loadData": {
    "params": {
      "dataName": "macho",
      "relativePath": "data/macho/macho-train.csv",
      "filter": [-99.0],
      "skip": true
    }
  },
  "extractFeatures": {
    "params": {
      "skip": true
    }
